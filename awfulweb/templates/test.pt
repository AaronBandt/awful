        <a href="/nodes">nodes</a>
      </div>
      <div class="menu_group">
        <h3>Groups</h3>
        <a href="/tags">tags</a><br>
        <a href="/graffitis">graffitis</a><br>
        <a href="/node_groups">node groups</a><br>
        <a href="/services">services</a><br>
        <a href="/load_balancer_pools">load balancer pools</a>
      </div>
      <div class="menu_group">
        <h3>Network</h3>
        <a href="/vips">VIPs</a><br>
        <a href="/ip_addresses">IP addresses</a><br>
        <a href="/network_ports">network ports</a><br>
        <a href="/subnets">subnets</a>
      </div>
      <div class="menu_group">
        <h3>Storage</h3>
        <a href="/drives">drives</a><br>
        <a href="/volues">volumes</a><br>
        <a href="/storage_controllers">storage controllers</a>
