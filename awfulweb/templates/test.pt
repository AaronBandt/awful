<html lang="en-US"
      xmlns="http://www.w3.org/1999/xhtml">
  <head profile="http://www.w3.org/2005/10/profile">
    <title>Arsenal</title>
    <link href="/static/test.css" rel="stylesheet" type="text/css" />
  </head>
  <body>

    <script src="/static/jquery-2.1.3.min.js"></script>
    <script src="/static/jquery-ui.min.js"></script>
    <script src="/static/jquery.dragtable.js"></script>
    <script type="text/javascript">
      $(document).ready(function() {

        $('#menu_btn_container').click(function(){
          $('#menu').toggle("slow");
        });


        $("#searchResultsTable").tablesorter({
            // set forced sort on the fourth column and i decending order.
            sortList: [[0,0]]
        });
        $('#searchResultsTable').dragtable({
            maxMovingRows:1,
            dragHandle:'.drag-handle',
            persistState: function(table) {
              if (!window.sessionStorage) return;
              var ss = window.sessionStorage;
              table.el.find('th').each(function(i) {
                if(this.id != '') {table.sortOrder[this.id]=i;}
              });
              ss.setItem('tableorder',JSON.stringify(table.sortOrder));
            },
            restoreState: eval('(' + window.sessionStorage.getItem('tableorder') + ')')
        });
      });
    </script>
    <script src="/static/jquery.tablesorter.min.js"></script>

  <div id='top'>

    <div id='logo_container'>
      <a href='#'><img src="/static/arsenal.png"></a>
    </div>
    <div id='menu_btn_container'>
      <div id="menu_btn">Menu</div>
    </div>

    <div id='search_container'>
      <form>
        <input type="text" class="search" placeholder="Search..." required>
        <input type="button" class="button" value="Search">
